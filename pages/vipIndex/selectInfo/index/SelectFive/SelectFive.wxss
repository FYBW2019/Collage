@import '/style/weui.wxss';
.weui-cell{
  border-bottom: 1px solid #aaa;
  background: rgba(216, 213, 213,0.3);
  font-family: "楷体";

}
.bton{
  width: 90%;
  border: 2px solid #115bfc79;
  border-radius: 10px;
  font-family: "楷体";
}
.bton3{
  width: 80%;
  border: 3px solid #ee9209b6;
  border-radius: 10px;
  font-family: "楷体";
}
.weui-flex{
  margin-top: 20px;
}
.parent {
  width: 100%;
  margin-top: 20px;
}

/* 表格代码   */

.table {
  display: flex;
}

.tr {
  width: 20%;
  display: block;
  float: left;
  justify-content: space-between;
}

.trr {
  display: block;
  float: left;
}

.th {
  padding: 10px 0 10px 0;
  border-bottom: 1px solid #dadada;
  border-right: 1px solid #dadada;
  text-align: center;
  width: 100%;
  overflow: hidden;
  font-weight: 800;
  background-color: #b663423b;
  font-size: 28rpx;
  color: #330e0e;
}

.td {
  width: 100px;
  padding: 13px 0 12px 0;
  border-bottom: 1px solid #dadada;
  border-right: 1px solid #dadada;
  text-align: center;
  color: #ec9480;
  background-color: #42b66923;
  font-size: 23rpx;

}
.pl{
  text-align: center;
}
.put{
  width: 90%;
  border: 2px solid #115bfc79;
  border-radius: 10px;
  font-family: "楷体";
  background-color: #ffffff;
  height: 46px;
margin: 0 auto;
}


