@import '/style/weui.wxss';
.weui-cell{
   border-top: 1px solid #aaa;
  border-bottom: 1px solid #aaa;
  background: rgba(216, 213, 213,0.1);
  font-family: "楷体";

}
.pageHander{
  width: 100%;
  height: 80px;
  border: 3px solid #115bfc79;
  border-radius: 10px;
  text-align: center;
  background: rgba(63, 206, 224, 0.1);
  padding: 30px 0 1px 0
}
.pageHander>text{
  font-size: 30px;
font-family: "楷体";
  letter-spacing:15rpx;
  
}
.history-table-wrap{
  width: 100%;
  height: 578rpx;
  overflow-y: scroll;
  overflow-x: hidden;
}
.pl{
   font-family: "楷体";
   color: #1a0303;
}
.put{
  margin: 0 auto;
  width: 90%;
  border: 1px solid #0e0e0ff5;
  border-radius: 10px;
  font-family: "楷体";
  height: 46px;
  text-align: center;
  z-index:-10;
  background-color: #ffffff;
  
}
.bton2{
 width: 90%;
 
  border-radius: 10px;
  font-family: "楷体";
}
.bton3{
  width: 80%;
  border: 1px solid #ee9209b6;
  border-radius: 10px;
  font-family: "楷体";
}
.weui-flex{
  margin-top: 20px;
}


.page__bd_spacing{
  width: 90%;
  margin: 0 auto;
  overflow: hidden;
  position: absolute;
  top:0px;
}
.show2{
    border-bottom: 2px solid #1a0303;
}
.placeholder{
 text-align: center;
 padding: 10px 0;
 
}
.section1{
  width: 100%;
  overflow: hidden;
}
.hander{
  width: 99%;
  margin: 0 auto;
  height: 100px;
  border: 1px #1a0303 solid;
  border-radius: 20px;
  margin-top: 10px;
}
.hander>text{
border: 1px solid salmon;
overflow: hidden;
width: 100px;
text-align: center;
float: left;
background-color: #ec9480;
margin-left: 5px;
font-size: 15px;
display: block;
padding: 5px 0 0 0;
border-radius: 10px;
text-justify: inter-cluster;
font-family: "楷体";
margin-top: 5px;
}
.leftList{
  width: 100%;
  height: 40px;
  text-align: center;
  padding: 12px 0 0 0;
  border-bottom: 1px solid #cccccc;
  border-right: 1px solid #cccccc;
}
.section{
  margin-top: 5px;
  width: 99%;
  overflow: hidden;
  border: 1px solid #cccccc;
   font-family: "楷体";
  }
.left{
  width: 49%;
  overflow: hidden;
  float: left;
 border-right: 1px solid #cccccc;

  
}
.check{
  width: 30%;
  height: 100%;float: left;
}
.te{
  width: 70%;
  height: 100%;
  float: right;
}
.show{
  width: 50%;
  overflow: hidden;
  float: right;
}
.RightList{
   width: 100%;
  height: 40px;
  text-align: center;
  padding: 12px 0 0 0;
  border-right: 1px solid #cccccc;
  border-bottom: 1px solid #cccccc;
}
.Table{
  width: 100%;
  overflow: hidden;
  margin-top: 20px;
}
/* 表格代码   */
.table{
  border:1px solid #dadada;
  border-right:0;
  border-bottom: 0;
  width: 100%;
}
.tr{
  width:100%;
  display: flex;
  justify-content: space-between;
}
.th,.td{
  padding: 10px;
  border-bottom: 1px solid #dadada;
  border-right: 1px solid #dadada;
  text-align: center;
  width: 100%;
}
.th1,.th2,.td1,.td2{
  width: 40%;
}
.th{
  font-weight: 800;
  font-size: 28rpx;
  color: #330e0e;
  background-color: #b663423b;
}
.td{
  font-size: 20rpx;
  color: #ec9480;
}

.txt{
  display: flex;
  align-items:center;
}
.weui-btn1{
  border: 1px seagreen solid;
  float: left;
  width: 40%;
}
.weui-btn2{
  border: 1px seagreen solid;
  float: right;
  width: 40%;
}
