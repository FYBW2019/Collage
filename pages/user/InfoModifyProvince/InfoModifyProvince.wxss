.nav{
  width: 100%;
  height: 180px;
  box-sizing: border-box;
  margin-bottom: 30px;
}
.BackImage{
  width: 100%;
  height: 100%;
}
.page1{
  width: 100%;
  height: 80px;
  position: absolute;
  box-sizing: border-box;
  top:10px;
  left: 0;
  text-align: center;
}
.UserImage{
  width: 70px;
  height: 70px;
  border-radius: 50%;
}
.page2{
  width: 100%;
  height: 20px;
  position: absolute;
  box-sizing: border-box;
  top:90px;
  left: 0;
  text-align: center;
  color: white;
}
.page3{
  width: 100%;
  height: 60px;
  position: absolute;
  box-sizing: border-box;
  top:120px;
  left: 0;
  text-align: center;
}
.page3Box{
  width: 33%;
  float: left;
  height: 60px;
  box-sizing: border-box;
}
.info1{
  width: 100%;
  height: 30px;
  text-align: center;
   box-sizing: border-box;
}
.info1>image{
  width: 30px;
  height: 30px;
}
.info2{
  width: 100%;
  height: 30px;
  text-align: center;
   box-sizing: border-box;
   color: white;
   font-size: 13px;
   line-height: 30px;
}
.show{
  width: 80%;
  margin: 0 auto;
  height: 150px;
  box-sizing: border-box;

}