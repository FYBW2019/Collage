.nav{
  width: 100%;
  height: 180px;
  box-sizing: border-box;
}
.BackImage{
  width: 100%;
  height: 100%;
}
.page1{
  width: 100%;
  height: 80px;
  position: absolute;
  box-sizing: border-box;
  top:10px;
  left: 0;
  text-align: center;
}
.UserImage{
  width: 70px;
  height: 70px;
  border-radius: 50%;
}
.page2{
  width: 100%;
  height: 20px;
  position: absolute;
  box-sizing: border-box;
  top:90px;
  left: 0;
  text-align: center;
  color: white;
}
.page3{
  width: 100%;
  height: 60px;
  position: absolute;
  box-sizing: border-box;
  top:120px;
  left: 0;
  text-align: center;
}
.page3Box{
  width: 33%;
  float: left;
  height: 60px;
  box-sizing: border-box;
}
.info1{
  width: 100%;
  height: 30px;
  text-align: center;
   box-sizing: border-box;
}
.info1>image{
  width: 30px;
  height: 30px;
}
.info2{
  width: 100%;
  height: 30px;
  text-align: center;
   box-sizing: border-box;
   color: white;
   font-size: 13px;
   line-height: 30px;
}
.menuList{
  width: 100%;
  height: 90px;
  box-sizing: border-box;
   margin-bottom: 50px;
}
.menu{
  width: 33%;
  height: 90px;
  float: left;
  box-sizing: border-box;
 
}
.menuImage{
  width: 100%;
  height: 60px;
  box-sizing: border-box;
  text-align: center;
}
.menuImage image{
  width: 60px;
  height: 60px;
}
.menuName{
  width: 100%;
  height: 30px;
  box-sizing: border-box;
   text-align: center;
   font-size: 15px;
   line-height: 30px;
   color: rgba(180, 172, 172, 0.815);
}

.menuInfo{
  width: 100%;
  height: 50px;
  box-sizing: border-box;
}
.menuLeft{
  width: 70%;
  height: 50px;
  line-height: 50px;
  box-sizing: border-box;
  margin-left: 10%;
  float: left;
  font-size: 16px;
  color: rgba(90, 86, 86, 0.733);
}
.menuRight{
  width: 10%;
  height: 50px;
  line-height: 50px;
  padding: 5px 0px;
  box-sizing: border-box;
  float: left;
}
.menuRight image{
  width: 15px;
  height: 15px;
}
.selected{
  border: 1px solid rgba(238, 81, 9, 0.644);
  color: rgba(238, 81, 9, 0.644);
}